{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/static/*", "/assets/*", "favicon.ico"]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
